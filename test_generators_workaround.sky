# Test Generators - Workaround (returns list)

function counter(max): any
  let results = []
  let i = 0
  while i < max
    results.append(i)
    i = i + 1
  end
  return results
end

function fibonacci(n): any
  let results = []
  let a = 0
  let b = 1
  let i = 0
  while i < n
    results.append(a)
    let temp = a
    a = b
    b = temp + b
    i = i + 1
  end
  return results
end

function main
  print("=== Generator Workaround Test ===")
  
  # Test counter
  print("Counter:")
  let counts = counter(5)
  for num in counts
    print("  " + str(num))
  end
  
  # Test fibonacci
  print("Fibonacci (first 10):")
  let fibs = fibonacci(10)
  for fib in fibs
    print("  " + str(fib))
  end
  
  print("=== Test Complete ===")
end
