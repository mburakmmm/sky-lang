# Lambda Function Test - Simple

function main
  print("=== Lambda Function Test ===")
  
  # Test lambda function syntax - simplified
  let square = function(x) x * x end
  print("Lambda created")
  
  # Test lambda call
  let result = square(5)
  print("5 squared = " + str(result))
  
  # Test lambda with multiple parameters
  let add = function(a, b) a + b end
  let sum = add(3, 4)
  print("3 + 4 = " + str(sum))
  
  # Test lambda in array
  let funcs = [square, add]
  print("Functions in array created")
  
  print("=== Test Complete ===")
end
