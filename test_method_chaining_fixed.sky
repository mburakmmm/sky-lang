# Method Chaining Test - Fixed

class Builder
  function init
    self.value = ""
  end
  
  function add(part): any
    self.value = self.value + part
    return self
  end
  
  function get_value: string
    return self.value
  end
end

function main
  print("=== Method Chaining Test - Fixed ===")
  
  let builder = Builder()
  print("Builder created")
  
  # Test method chaining
  let result = builder.add("Hello").add(" ").add("World").get_value()
  print("Chained result: " + result)
  
  # Test direct method calls
  let builder2 = Builder()
  builder2.add("SKY")
  builder2.add(" Language")
  print("Direct calls: " + builder2.get_value())
  
  print("=== Test Complete ===")
end
