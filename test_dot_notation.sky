# Test Dot Notation

class Person
  function init(name, age): any
    self.name = name
    self.age = age
    self.address = {"city": "Istanbul", "country": "Turkey"}
    return self
  end
  
  function get_name: string
    return self.name
  end
  
  function get_address: any
    return self.address
  end
end

function main
  print("=== Dot Notation Test ===")
  
  let person = Person("Ali", 25)
  
  # Test simple dot notation
  print("Name: " + person.name)
  print("Age: " + str(person.age))
  
  # Test method call
  print("get_name(): " + person.get_name())
  
  # Test nested dot notation (obj.prop.prop)
  let city = person.address.get("city")
  print("City: " + city)
  
  # Test obj.method().method()
  let text = "hello"
  let result = text.upper().lower()
  print("Chained string methods: " + result)
  
  print("=== Test Complete ===")
end
