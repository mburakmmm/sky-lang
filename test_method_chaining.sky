# Method Chaining Test

class Builder
  function init
    self.value = ""
  end
  
  function add(part): any
    print("add method called with: " + part)
    self.value = self.value + part
    print("self.value is now: " + self.value)
    return self
  end
  
  function get_value: string
    print("get_value method called")
    print("self.value is: " + self.value)
    return self.value
  end
end

function main
  print("=== Method Chaining Testi ===")
  
  let builder = Builder()
  print("Builder created")
  
  # Test single method call first
  builder.add("Hello")
  print("Added Hello")
  
  let value = builder.get_value()
  print("Value: " + value)
  
  # Test direct method calls
  let builder2 = Builder()
  builder2.add("SKY")
  builder2.add(" Language")
  print("Direct calls: " + builder2.get_value())
  
  print("=== Test TamamlandÄ± ===")
end
