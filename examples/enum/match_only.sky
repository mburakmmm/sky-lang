import _enum_bypass

enum Color
  Red
  Green
end

function test_match(c)
  match c
    Red() => print("  Red in function")
    Green() => print("  Green in function")
  end
end

function main
  print("1. Before creating")
  let c = Red()
  print("2. After creating")
  test_match(c)
  print("3. After match function")
end

