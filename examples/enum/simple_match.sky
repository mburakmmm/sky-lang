import _enum_bypass

enum Color
  Red
  Green
end

function main
  let c = Red()
  print("Before match")
  
  match c
    Red() => print("Red matched")
    Green() => print("Green matched")
  end
  
  print("After match")
  
  let c2 = Green()
  print("Created c2")
  
  match c2
    Red() => print("Red matched 2")
    Green() => print("Green matched 2")
  end
  
  print("Done!")
end

