# Debug payload binding
import _enum_bypass

enum Shape
  Circle(int)
  Rectangle(int, int)
end

function main
  let c = Circle(5)
  let r = Rectangle(10, 20)
  
  print("Testing Circle:")
  match c
    Circle(radius) => print("  Radius =", radius)
    Rectangle(w, h) => print("  Not rectangle")
  end
  
  print("Testing Rectangle:")
  match r
    Circle(radius) => print("  Not circle")
    Rectangle(width, height) => print("  Width =", width, "Height =", height)
  end
end

