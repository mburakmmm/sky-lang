# Function parsing examples

function simple
  print("hello")
end

function withParams(a, b, c)
  return a + b + c
end

function withTypes(x: int, y: float): float
  return x + y
end

function withDefaults(name, greeting = "Hello")
  print(greeting + " " + name)
end

async function asyncExample
  let result = await fetchData()
  return result
end

