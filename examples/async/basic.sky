# Basic async/await example

async function fetchData(id: int)
  print("Fetching data for ID: " + id)
  # Simulate async I/O
  return id * 10
end

async function processData
  let data1 = await fetchData(1)
  print("Received: " + data1)
  
  let data2 = await fetchData(2)
  print("Received: " + data2)
  
  return data1 + data2
end

function main
  let result = await processData()
  print("Final result: " + result)
end

