# Enum with payload test

enum Shape
  Circle(int)
  Rectangle(int, int)
end

function describe(shape)
  match shape
    Circle(r) => print("Circle with radius:", r)
    Rectangle(w, h) => print("Rectangle:", w, "x", h)
  end
end

function main
  let c = Circle(5)
  let r = Rectangle(10, 20)
  
  describe(c)
  describe(r)
  
  print("Test complete!")
end
