import _native_bypass

function main
  print("=== Testing Native FS Functions ===")
  
  let path = "test_file.txt"
  let content = "Hello from Sky stdlib!"
  
  print("1. Writing file...")
  let write_ok = fs_write_text(path, content)
  print("   Write result:", write_ok)
  
  print("2. Checking if exists...")
  let exists = fs_exists(path)
  print("   Exists:", exists)
  
  print("3. Reading file...")
  let read_content = fs_read_text(path)
  print("   Content:", read_content)
  
  print("4. Getting platform...")
  let platform = os_platform()
  print("   Platform:", platform)
  
  print("5. Getting cwd...")
  let cwd = os_getcwd()
  print("   CWD:", cwd)
  
  print("6. Testing crypto...")
  let hash = crypto_sha256("test")
  print("   SHA256:", hash)
  
  print("7. Testing JSON...")
  let data = {"name": "Sky"}
  let json_str = json_encode(data)
  print("   JSON:", json_str)
  
  print("8. Testing time...")
  let now = time_now()
  print("   Timestamp:", now)
  
  print("9. Testing rand...")
  let uuid = rand_uuid()
  print("   UUID:", uuid)
  
  print("")
  print("ALL NATIVE STDLIB FUNCTIONS WORKING!")
end
