import std.collections.set

function main
  let s1 = Set([1, 2, 3, 3, 2])
  print("Set 1:", s1.to_list())
  print("Length:", s1.len())
  
  let s2 = Set([3, 4, 5])
  print("Set 2:", s2.to_list())
  
  let union = s1.union(s2)
  print("Union:", union.to_list())
  
  let intersection = s1.intersection(s2)
  print("Intersection:", intersection.to_list())
  
  let diff = s1.difference(s2)
  print("Difference:", diff.to_list())
end

