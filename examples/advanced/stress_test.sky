# SKY Stress Test - Tüm Özellikler

function fib(n: int): int
  if n <= 1
    return n
  end
  return fib(n - 1) + fib(n - 2)
end

function factorial(n: int): int
  if n <= 1
    return 1
  end
  return n * factorial(n - 1)
end

function isPrime(n: int): bool
  if n <= 1
    return false
  end
  if n == 2
    return true
  end
  if n % 2 == 0
    return false
  end
  let i = 3
  while i * i <= n
    if n % i == 0
      return false
    end
    i = i + 2
  end
  return true
end

function sumRange(n: int): int
  let sum = 0
  for i in range(n)
    sum = sum + i
  end
  return sum
end

