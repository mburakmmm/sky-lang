# Simple function examples

function add(a, b)
  return a + b
end

function multiply(x, y)
  return x * y
end

function greet(name)
  print("Hello,", name, "!")
end

function main
  print("=== SIMPLE FUNCTIONS ===")
  
  let sum = add(10, 20)
  print("10 + 20 =", sum)
  
  let product = multiply(5, 6)
  print("5 * 6 =", product)
  
  greet("Sky")
end

