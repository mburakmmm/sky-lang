# TEST: Async/Await Features
# Expected: Should work according to .cursorrules

async function fetchData(id: int): int
  await delay(100)
  return id * 2
end

async function processData
  let result1 = await fetchData(10)
  let result2 = await fetchData(20)
  return result1 + result2
end

function delay(ms: int): int
  return ms
end

function main
  let data = await processData()
  print("Async result: " + data)
end

